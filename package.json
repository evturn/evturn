{
  "name": "evturn.com",
  "version": "2.0.1",
  "description": "A website about me for you.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development babel-node server.js",
    "clean": "rimraf ./build",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack --config webpack.config.js --color -p",
    "postbuild": "open index.html"
  },
  "dependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-router": "4.0.0-alpha.6",
    "rxjs": "^5.0.0-beta.11",
    "sanitize.css": "^4.1.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.13.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.26.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.0.0",
    "imports-loader": "^0.7.0",
    "json-loader": "0.5.4",
    "koa": "v2.0.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-focus": "1.0.0",
    "postcss-loader": "^1.2.0",
    "postcss-reporter": "^2.0.0",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.15",
    "write-file-webpack-plugin": "^3.3.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-function-bind"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "repository": "git@github.com:evturn/evturn.github.io.git",
  "author": "Evan Turner <ev@evturn.com>",
  "license": "MIT"
}

{
  "name": "evturn.com",
  "version": "2.0.0",
  "description": "A website about me for you.",
  "main": "",
  "scripts": {
    "start": "NODE_ENV=development webpack-dashboard -- babel-node config/server.js",
    "build": "NODE_ENV=production webpack --config config/webpack.prod.babel.js --color -p",
    "postbuild": "open index.html",
    "fetch": "node config/build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evturn/evturn.github.io.git"
  },
  "author": {
    "name": "Evan Turner",
    "url": "http://evturn.com",
    "github": "http://github.com/evturn",
    "npm": "https://www.npmjs.com/~evturn"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/evturn/evturn.github.io/issues"
  },
  "homepage": "https://github.com/evturn/evturn.github.io",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-function-bind"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "history": "^2.0.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "react-router-scroll": "^0.2.1",
    "redux": "^3.6.0",
    "redux-observable": "^0.10.0",
    "rxjs": "^5.0.0-beta.11",
    "sanitize.css": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "chalk": "^1.1.3",
    "clean-webpack-plugin": "^0.1.10",
    "cloudinary": "^1.4.2",
    "css-loader": "0.23.1",
    "exports-loader": "0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "mkdirp": "^0.5.1",
    "null-loader": "0.1.1",
    "postcss-cssnext": "2.7.0",
    "postcss-focus": "1.0.0",
    "postcss-loader": "0.9.1",
    "postcss-modules-values": "^1.2.2",
    "postcss-reporter": "1.4.1",
    "redux-logger": "^2.6.1",
    "request": "^2.74.0",
    "rimraf": "^2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.15",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "write-file-webpack-plugin": "^3.3.0"
  }
}

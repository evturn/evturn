{
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 1,
    "eqeqeq": 1,
    "global-strict": 0,
    "no-console": 1,
    "no-extra-semi": 1,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-undef": 1,
    "no-underscore-dangle": 0,
    "no-unreachable": 1,
    "semi": 2,
    "quotes": [1, "single"]
  },
  "globals": {
    "jQuery": true,
    "$": true,
    "_": true
  }
}